header{
			width:960px;
			height:100px;
			border:1px solid red;
			border-radius:10px;
			margin:20px auto;
			
}
nav.top_menu{
			width:960px;
			height:300px;
			border-radius:10px;
			margin:20px auto;
			float:center;

}
nav.top_menu ul li{		display:inline;
						position: relative;


}



nav.top_menu ul li a{
			display:inline-block;
			padding:0 50px;
			height:50px
			line-height:60px;
			color:#CCCC66;
			font-size:150px
			font-weight:500px;
			text-decoration:none;
            text-align: center;
            border-right: solid 1px #834524;
            color: #ffffff;
            transition:background 0.5s linear;
			
}
nav.top_menu ul ul{
			visibility: hidden;
			position: absolute;
			left:-1px;
}
nav.top_menu ul li:hover ul{visibility: visible; z-index: 100;}

main{
			width:960px;
			height:1550px;
			
			border-radius:10px;
			margin:20px auto;

}
